<h1><%= @team.name %></h1>

<h3>Lineup:</h3>

<% @team.players.each do |player| %>
    <p>
        <%= link_to player.name, player_path(player.id) %>
    </p>
<% end %>

<div class="controls">
    <%= link_to "Edit Starting 5", edit_team_path(@team.id) %>
    |
    <%= link_to "Delete Starting 5", @team, :method => :delete, :data => { :confirm => 'Are you sure?' } %>
</div>